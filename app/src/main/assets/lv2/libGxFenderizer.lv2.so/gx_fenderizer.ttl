<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:guitarix-developer@lists.sourceforge.net> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_fenderizer_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_fenderizer_" .

<http://guitarix.sourceforge.net/plugins/gx_fenderizer_#_fenderizer_>
    a lv2:Plugin ,
        lv2:FilterPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxFenderizer";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_fenderizer_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
      
    lv2:minorVersion 35;
    lv2:microVersion 0;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_fenderizer_gui#_fenderizer_>;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "BYPASS" ;
        lv2:name "BYPASS" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:designation lv2:enabled;
        lv2:portProperty lv2:integer;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "BASS" ;
        lv2:name "BASS" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "BRIGHT" ;
        lv2:name "BRIGHT" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "Solo"; rdf:value 0];
        lv2:scalePoint [rdfs:label "Bright"; rdf:value 1];
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "MID" ;
        lv2:name "MID" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "TREBLE" ;
        lv2:name "TREBLE" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "VOLUME" ;
        lv2:name "VOLUME" ;
        lv2:default 0.15 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "CLEAN" ;
        lv2:name "CLEAN" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "CRUNCH"; rdf:value 0];
        lv2:scalePoint [rdfs:label "CLEAN"; rdf:value 1];
    ] .

<http://guitarix.sourceforge.net/plugins/gx_fenderizer_gui#_fenderizer_>
  a guiext:X11UI;
  guiext:binary <gx_fenderizer_ui.so>;
        lv2:extensionData guiext::idle ; 
        lv2:requiredFeature guiext:makeResident;
  .
