
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix opts:   <http://lv2plug.in/ns/ext/options#> .
@prefix time:   <http://lv2plug.in/ns/ext/time#>.
@prefix units:  <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:   <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:   <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:   <http://lv2plug.in/ns/ext/midi#> .
@prefix patch:  <http://lv2plug.in/ns/ext/patch#> .


<urn:name#me>
   a foaf:Person ;
   foaf:name "brummer" .

<urn:brummer:bluesbreaker>
   a lv2:Plugin ,
       lv2:MixerPlugin ;
   doap:maintainer <urn:name#me> ;
   doap:name "bluesbreaker" ;
   lv2:project <urn:brummer:bluesbreaker> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:minorVersion 1 ;
   lv2:microVersion 0 ;

guiext:ui <urn:brummer:bluesbreaker_ui> ;

rdfs:comment """
...
""";


   lv2:port  [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 0 ;
      lv2:symbol "in0" ;
      lv2:name "In0" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 1 ;
      lv2:symbol "out0" ;
      lv2:name "Out0" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 2 ;
      lv2:designation lv2:enabled;
      lv2:portProperty lv2:toggled ;
      lv2:symbol "Bypass" ;
      lv2:name "bypass" ;
      lv2:default 1 ;
      lv2:minimum 0 ;
      lv2:maximum 1 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 3 ;
      lv2:symbol "GAIN" ;
      lv2:name "gain" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 4 ;
      lv2:symbol "TONE" ;
      lv2:name "tone" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "VOLUME" ;
      lv2:name "volume" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
   ] .


<urn:brummer:bluesbreaker_ui>
   a guiext:X11UI;
   guiext:binary <bluesbreaker_ui.so> ;
       lv2:extensionData guiext::idle ;
       lv2:extensionData guiext:resize ;
       lv2:extensionData guiext:idleInterface ;
       lv2:requiredFeature guiext:idleInterface ;
       lv2:optionalFeature opts:options ;
       opts:supportedOption guiext:scaleFactor ; .
